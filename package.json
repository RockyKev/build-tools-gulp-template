  "version": "1.0.0",
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "10.4.x",
