    "eslint-config-htmlacademy": "8.0.x",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-csso": "^4.0.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "9.0.x",
    "gulp-posthtml": "^3.0.5",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "3.0.x",
    "gulp-svgstore": "^9.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-webp": "^4.0.1",
    "pixel-glass": "^1.0.13",
    "posthtml-include": "^1.7.4",
    "sass": "^1.56.1",
    "stylelint": "^14.16.0",
    "stylelint-config-htmlacademy": "1.1.x"
